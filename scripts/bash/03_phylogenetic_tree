#new folder
mkdir phylo_tree

# create file list w paths to .faa files
find ../prokka_out/prokka_*/ -name "*.faa" > faa_list.txt

# download archaea hmm set (not needed when using default) 
wget https://zenodo.org/records/13858489/files/Archaea.hmm?download=1
mv Archaea.hmm?download=1 Archaea.hmm #easier to recognize

# run gtotree
## july 19
GToTree -A faa_list.txt -H Archaea -j 32 -o archaeal_tree #uses default Archaea set 
